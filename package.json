{
  "name": "babel-ava-metalsmith",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run dev",
    "dev": "rimraf dist/** && npm-run-all -p dev:*",
    "dev:Js": "NODE_ENV=development webpack -w --config ./config/webpack.config.js",
    "dev:css": "postcss --dir ./dist/css/ ./src/css/*.css -w -c ./config/postcss.config.js",
    "dev:html": "node_modules/.bin/metalsmith -c ./config/metalsmith-dev.json",
    "dev:copystub": "cpx \"src/stub/**\" \"dist\" --watch",
    "dev:Server": "php -S 127.0.0.1:5000 -t=./dist",
    "dev:livereload": "browser-sync start --proxy \"127.0.0.1:5000\" --files \"./dist/css/style.css, ./dist/js/*.js, ./dist/*.html\" --port 4000",
    "front": "rimraf dist/** && npm-run-all -p front:*",
    "front:Js": "NODE_ENV=production webpack --config ./config/webpack.config.js",
    "front:css": "postcss --dir ./dist/css/ ./src/css/*.css -c ./config/postcss.config.js",
    "front:html": "node_modules/.bin/metalsmith -c ./config/metalsmith-prod.json",
    "front:stub": "cpx \"src/stub/**\" \"dist/stub\"",
    "prod": "rimraf dist/** && npm-run-all -p prod:*",
    "prod:Js": "NODE_ENV=production webpack --config ./config/webpack.config.js",
    "prod:css": "postcss --dir ./dist/css/ ./src/css/*.css -c ./config/postcss.config.js",
    "prod:html": "node_modules/.bin/metalsmith -c ./config/metalsmith-prod.json",
    "spec": "npm-run-all -p spec:*",
    "spec:delcoverage": "rimraf test/coverage",
    "spec:unit": "nyc ava",
    "addImages": "imagemin pngquant ./src/images/* -o ./dist/images/"
  },
  "ava": {
    "babel": "inherit",
    "verbose": true,
    "require": [
      "babel-register",
      "./test/helpers/setup.js"
    ],
    "files": [
      "./test/unit/**/*.js"
    ]
  },
  "nyc": {
    "report-dir": "test/coverage",
    "reporter": [
      "html",
      "lcov",
      "text"
    ],
    "extension": [
      ".js"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "ava": "^0.22.0",
    "axios-mock-adapter": "^1.10.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-env": "^3.2.0",
    "chokidar-cli": "^1.2.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.5",
    "cssnano": "^3.10.0",
    "es6-promise": "^4.1.0",
    "eslint": "^3.3.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.10",
    "imagemin-cli": "^3.0.0",
    "imagemin-pngquant": "^5.0.1",
    "lite-server": "^2.3.0",
    "metalsmith": "^2.1.0",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-in-place": "^1.4.4",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-partial": "^0.1.0",
    "metalsmith-watch": "^1.0.3",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.2.1",
    "postcss-assets": "^5.0.0",
    "postcss-cli": "^4.1.1",
    "postcss-custom-properties": "^6.1.0",
    "postcss-easy-import": "^2.1.0",
    "postcss-extend": "^1.0.5",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.1.2",
    "postcss-plugin-bem-escape-block-name-less-modifier": "^0.1.1",
    "postcss-sassy-mixins": "^2.1.0",
    "postcss-sorting": "^3.0.1",
    "require-extension-hooks": "^0.3.0",
    "require-extension-hooks-babel": "^0.1.1",
    "rewire": "^3.0.0",
    "rimraf": "^2.6.1",
    "sinon": "^3.3.0",
    "uglify-js": "^3.0.27",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "qs": "^6.5.1"
  }
}
